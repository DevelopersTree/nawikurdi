{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www & npx parcel watch public/js/main.js --out-dir public/js/dist --no-source-maps",
    "dbMigration": "npx knex migrate:latest --knexfile ./database/knexfile.js",
    "dbSeeds": "npx knex seed:run --knexfile ./database/knexfile.js",
    "parcel:build": "sudo npx parcel build public/javascripts/main.js --out-dir public/javascripts/dist"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "dotenv": "6.2.0",
    "ejs": "^3.1.3",
    "express": "4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-validator": "6.1.1",
    "helmet": "^3.22.0",
    "http-errors": "^1.7.3",
    "jquery": "^3.5.1",
    "knex": "0.19.5",
    "morgan": "1.9.1",
    "mysql": "2.17.1",
    "mysql2": "1.6.5",
    "serve-favicon": "^2.5.0",
    "slugify": "^1.4.0",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "eslint": "6.2.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "parcel": "^1.12.4"
  }
}
